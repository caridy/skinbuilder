{{prefix}}{{skinName}} {{prefix}}calendar-content {
    padding: {{containerPadding}};
    border-radius: {{containerRadius}};
    {{gradient}};
    background-color: {{background}};
    color: {{text}};
    border: solid 1px;
    border-color: {{borderHigh}} {{borderLow}} {{borderLow}} {{borderHigh}};
}

{{prefix}}{{skinName}} {{prefix}}calendar-grid {
    padding: 5px;  /* any affect? */
    border-collapse: collapse;
}

{{prefix}}{{skinName}} {{prefix}}calendar-header {
    padding-bottom:10px;
    color: {{headerText}};
}

{{prefix}}{{skinName}} {{prefix}}calendar-header-label {
    margin: 0;
    font-size: 1em;
    font-weight: bold;
}


{{prefix}}{{skinName}} {{prefix}}calendar-day,
{{prefix}}{{skinName}} {{prefix}}calendar-prevmonth-day,
{{prefix}}{{skinName}} {{prefix}}calendar-nextmonth-day {
    padding: {{dayPadding}};
    text-align: center;
}




{{prefix}}{{skinName}} {{prefix}}calendar-weekday {
    font-weight: bold;
    background: {{weekdayHeadBackground}};
    color: {{weekdayHeadText}};
}

{{prefix}}{{skinName}} {{prefix}}calendar-prevmonth-day,
{{prefix}}{{skinName}} {{prefix}}calendar-nextmonth-day {
    background: {{nextPrevMonthDayBackground}};
    color: {{nextPrevMonthDayText}};
    border: solid 1px {{dayBorder}};
}

{{prefix}}{{skinName}} {{prefix}}calendar-day {
    font-weight: bold;
    background: {{dayBackground}};
    color: {{dayText}};
    border: solid 1px {{dayBorder}};
}
{{prefix}}{{skinName}} {{prefix}}calendar-day:hover,         /* When cursor   is used to navigate days */
{{prefix}}{{skinName}} {{prefix}}calendar-day-highlighted {  /* When keyboard is used to navigate days */
    background: {{dayHoverBackground}};
    color: {{dayHoverText}};
}

{{prefix}}{{skinName}} {{prefix}}calendar-day-selected {
    {{daySelectedGradient}};
    background-color: {{daySelectedBackground}};
    color: {{daySelectedText}};
}

{{prefix}}{{skinName}} {{prefix}}calendar-day-selected:hover, /* When cursor hovers over selected days */
{{prefix}}{{skinName}} {{prefix}}calendar-day-selected{{prefix}}calendar-day-highlighted { /* When keyboard focuses over selected days */
    background-color: {{daySelectedHoverBackground}};
    color: {{daySelectedHoverText}};
}

{{prefix}}{{skinName}} {{prefix}}calendar-header-label {
    text-align: center;
}

{{prefix}}{{skinName}} {{prefix}}calendar-left-grid {
   margin-right:1em;
}

{{prefix}}{{skinName}} {{prefix}}calendar-right-grid {
   margin-left:1em;
}

{{prefix}}{{skinName}} {{prefix}}calendar-selection-disabled,
{{prefix}}{{skinName}} {{prefix}}calendar-selection-disabled:hover {
  color: {{dayDisabledText}};
  background: {{dayDisabledBackground}};
}
/**************** the following from calendarnavigator.css ******************/
{{prefix}}{{skinName}} {{prefix}}calendarnav-prevmonth, {{prefix}}{{skinName}} {{prefix}}calendarnav-nextmonth {
  color: #000000;  /*falls back to text if image not rendered */
  width: 12px;
  height: 14px;
  background: transparent url(data:image/png;base64,);
  background-repeat: no-repeat;
}

{{prefix}}{{skinName}} {{prefix}}calendarnav-prevmonth:hover, {{prefix}}{{skinName}} {{prefix}}calendarnav-nextmonth:hover {
  background: transparent url(data:image/png;base64,);
  color: #0066CC;
}

{{prefix}}{{skinName}} {{prefix}}calendarnav-month-disabled, {{prefix}}{{skinName}} {{prefix}}calendarnav-month-disabled:hover {
  background: transparent url(data:image/png;base64,);
  cursor: default;
  color: #CCCCCC;
}

{{prefix}}{{skinName}} {{prefix}}calendarnav-prevmonth, {{prefix}}{{skinName}} {{prefix}}calendarnav-prevmonth:hover {
  background-position: 0px 0px;
  margin-left:-12px;
}

{{prefix}}{{skinName}} {{prefix}}calendarnav-nextmonth, {{prefix}}{{skinName}} {{prefix}}calendarnav-nextmonth:hover {
  background-position: -12px 0px;
  margin-right: -12px;
}

{{prefix}}{{skinName}} {{prefix}}calendarnav-prevmonth span, {{prefix}}{{skinName}} {{prefix}}calendarnav-nextmonth span {
   display: none;
   *display: block;
}
