/*************** Begin styles for the UI display of the color space in this tool *************/
/* Not needed for a skin */
#tab-schemes,
#tab-modules,
.slider-markup-text-contrast,
.preview-label {
    color: {{blockPageTextNormal}};
}
.hint, .reset {
    color: {{blockPageTextLow}};
}

.help {
    background-color: {{blockPageTextLow}};
    color: {{blockPageBackground}};
}
.widget-preview-label {
    border: solid 1px {{blockPageBorderLow}};
    border-style: solid none none none;
    color: {{blockPageTextNormal}};
    font-size: 70%;
    font-style: normal;
    margin: 0 0 10px;
    height: 1.2em;
    overflow: hidden;
}

#textarea-style{
    color: {{blockPageTextHigh}};
}

.tab-code-textarea-label {
    color: {{blockPageTextNormal}};
}

#tabview-controls-container { /*needed a container to be 100% fixed so the tabview-controls could have left and right margin*/
    background-color: {{blockPageBackground}};
}

#tabview-controls .yui3-tabview-panel {
    border: solid 1px;
    border-color: {{blockPageBorderLow}};
    background-color: {{blockPageBackground}};
}
#tabview-controls-container label {
    color: {{blockPageTextNormal}};
}
#tab-modules #checkboxes-widget,
#tab-modules #checkboxes-yuicss  { /* group bottom border on checkboxes for modules/widgets */
    border-bottom: solid 1px {{blockPageRuleLow}};
    padding-bottom: 0.5em;
}
#tab-modules .query-mode-change a {
    color: {{blockPageTextHigh}};
}
#feedback-box {
    position: absolute;
    left: 0;
    top: 30px;
    border: solid 1px {{blockPageTextLow}};
    background-color: {{blockPageBorderLow}};
    padding: {{feedbackPadding}};
    border-radius: {{feedbackRadius}};
}
#feedback-box div {
    background-color: {{blockPageBackground}};
    color: {{blockPageTextNormal}};
}
.feedback-column {
    position: relative;
    width: 75px;
    height: 77px;
}
/*  Buttons for key color and page color editing */
#tab-palette .bucket-highest {
    {{blockHighestGradient}};
    background-color: {{blockHighestBackground}};
    color: {{blockHighestTextNormal}};
    border: solid 1px;
    border-color: {{blockHighestBorderHigh}} {{blockHighestBorderLow}} {{blockHighestBorderLow}} {{blockHighestBorderHigh}};
}
#tab-palette .bucket-highest:hover {
    {{blockHighestHoverGradient}};
    background-color: {{blockHighestHoverBackground}};
    color: {{blockHighestHoverTextNormal}};
    border: solid 1px;
    border-color: {{blockHighestHoverBorderHigh}} {{blockHighestHoverBorderLow}} {{blockHighestHoverBorderLow}} {{blockHighestHoverBorderHigh}};
    box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.4);
}
#tab-palette .bucket-page {
    {{blockPageGradient}};
    background-color: {{blockPageBackground}};
    color: {{blockPageTextNormal}};
    border: solid 1px;
    border-color: {{blockPageBorderHigh}} {{blockPageBorderLow}} {{blockPageBorderLow}} {{blockPageBorderHigh}};
}
#tab-palette .bucket-page:hover {
    {{blockPageHoverGradient}};
    
    background-color: {{blockPageHoverBackground}};
    color: {{blockPageHoverTextNormal}};
    border: solid 1px;
    border-color: {{blockPageHoverBorderHigh}} {{blockPageHoverBorderLow}} {{blockPageHoverBorderLow}} {{blockPageHoverBorderHigh}};
    box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.4);
}

#scrollview-content-horiz li {

    border-bottom-style: none;
    border-right: solid 1px {{ruleLow}};
}

/* labels need to be narrower in Skin Preview or they stack over inputs */
{{yuiCssPrefix}}{{skinName}} .sb-preview-form {{yuiCssPrefix}}form-aligned {{yuiCssPrefix}}control-group label {
    width: 8em;
}
{{yuiCssPrefix}}{{skinName}} .sb-preview-form {{yuiCssPrefix}}form-aligned {{yuiCssPrefix}}controls {
  margin: 1.5em 0 0 8.35em;
}


/* start elements in the palette */

{{prefix}}{{skinName}} .background {
    background-color: {{background}};
    border: solid 1px;
    border-color: {{borderHigh}} {{borderLow}} {{borderLow}} {{borderHigh}};
}

{{prefix}}{{skinName}} .text-low {
    color: {{textLow}};
}

{{prefix}}{{skinName}} .text-normal {
    color: {{textNormal}};
}

{{prefix}}{{skinName}} .text-high {
    color: {{textHigh}};
}

{{prefix}}{{skinName}} .rule-low {
    border-top: solid 1px {{ruleLow}};
}

{{prefix}}{{skinName}} .rule-high {
    border-top: solid 1px {{ruleHigh}};
}

{{prefix}}{{skinName}} .gradient {
    {{gradient}};
}
/* background:hover **************/
