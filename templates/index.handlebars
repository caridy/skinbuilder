<!-- YUICSS Markup -->
    <li class="sb-preview-form"><div class="widget-preview-label">Forms - Checkboxes and Radios</div>
        <form class="pure-form">
        <label class="pure-checkbox">
        <input type="checkbox" value="">
        Checkbox option 
        </label>
        <label class="pure-radio">
        <input type="radio" checked="" value="option1" id="optionsRadios1" name="optionsRadios">
        Radio button choice
        </label>
        <label class="pure-radio">
        <input type="radio" value="option2" id="optionsRadios2" name="optionsRadios">
        Radio other choice 
        </label>
        </form>
    </li>

    <li class="sb-preview-form double-wide"><div class="widget-preview-label">Default Inline Form</div>
        <form class="pure-form">
        <fieldset>
        <legend>Legend</legend>
        <input type="text" placeholder="Email">
        <input type="password" placeholder="Password">
        <label>
        <input type="checkbox"> Opt in
        </label>
        <button class="pure-button" type="submit">Sign in</button>
        </fieldset>
        </form>
    </li>

    <li class="sb-preview-form"><div class="widget-preview-label">Aligned Form</div>
        <form class="pure-form pure-form-aligned">
        <fieldset>
        <div class="pure-control-group">
        <label>Username</label>
        <input type="text" placeholder="Username">
        </div>

        <div class="pure-control-group">
        <label>Password</label>
        <input type="password" placeholder="Password">
        </div>

        <div class="pure-control-group">
        <label>Email Address</label>
        <input type="text" placeholder="Email Address">
        </div>

        <div class="pure-control-group">
        <label>Disabled</label>
        <input type="text" disabled="" placeholder="Disabled input here..."/>
        </div>

        <div class="pure-controls">
        <label class="pure-checkbox">
        <input type="checkbox"> Check me out
        </label>
        <button class="pure-button" type="submit">Submit</button>
        </div>
        </fieldset>
        </form>
    </li>        
    <li class="sb-preview-form"><div class="widget-preview-label">Form - Stacked</div>
        <form class="pure-form pure-form-stacked">
        <fieldset>
        <legend>Legend</legend>
        <label>Full Name</label>
        <input type="text">
        <aside class="pure-form-message">Block help text</aside>
        <label>E-Mail</label>
        <input type="email" required="">
        <aside class="pure-form-message-inline">Required</aside>
        <label>State</label>
        <select class="pure-input-medium">
        <option>AL</option>
        <option>CA</option>
        <option>IL</option>
        <option>MD</option>
        <option>NY</option>
        </select>
        <label class="pure-checkbox">
        <input type="checkbox"> I've read the terms and conditions
        </label>
        <button class="pure-button notice" type="submit">Submit</button>
        </fieldset>
        </form>        
    </li>
    <li class="sb-preview-form"><div class="widget-preview-label">Form - Grouped Inputs</div>
        <form class="pure-form">
        <fieldset class="pure-group">
        <input type="text" placeholder="Username" class="pure-input-1-2">
        <input type="text" placeholder="Password" class="pure-input-1-2">
        <input type="text" placeholder="Email" class="pure-input-1-2">
        </fieldset>
        <fieldset class="pure-group">
        <input type="text" placeholder="Another Group" class="pure-input-1-2">
        <input type="text" placeholder="More Stuff" class="pure-input-1-2">
        </fieldset>
        <button class="pure-button pure-input-1-2" type="submit">Sign in</button>
        </form>
    </li>        
    <li class="sb-preview-form"><div class="widget-preview-label">Rounded Form</div>
